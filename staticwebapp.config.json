{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/api/*", "/_content/*", "*.css", "*.js", "*.svg", "*.woff", "*.woff2"]
  },
  "routes": [
    {
      "route": "/management/*",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/api/management/*",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/_content/**",
      "headers": {
        "Cache-Control": "public, max-age=31536000"
      }
    }
  ],
  "mimeTypes": {
    ".css": "text/css",
    ".js": "application/javascript",
    ".woff": "font/woff",
    ".woff2": "font/woff2",
    ".svg": "image/svg+xml",
    ".json": "application/json",
    ".csv": "text/csv"
  },
  "globalHeaders": {
    "X-Content-Type-Options": "nosniff"
  }
}