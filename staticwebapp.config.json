{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/api/*",
      "/_content/**",
      "/_framework/**", 
      "/css/**",
      "/js/**",
      "*.css",
      "**/*.css", 
      "*.styles.css",
      "**/*.styles.css",
      "**/*.js",
      "**/*.svg",
      "**/*.woff",
      "**/*.woff2",
      "**/*.map",
      "**/*.json",
      "**/*.dat",
      "**/*.pdb",
      "**/*.dll",
      "**/*.wasm",
      "**/*.blat"
    ]
  },
  "routes": [
    {
      "route": "/management/*",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/api/management/*",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/api/database/*",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/_content/**",
      "allowedRoles": ["anonymous"],
      "headers": {
        "Cache-Control": "public, max-age=31536000",
        "Content-Type": "text/css"
      }
    },
    {
      "route": "/_content/**/*.css",
      "allowedRoles": ["anonymous"],
      "headers": {
        "Content-Type": "text/css"
      }
    },
    {
      "route": "/_framework/**",
      "allowedRoles": ["anonymous"],
      "headers": {
        "Cache-Control": "public, max-age=31536000"
      }
    }
  ],
  "mimeTypes": {
    ".css": "text/css",
    ".js": "application/javascript",
    ".woff": "font/woff",
    ".woff2": "font/woff2",
    ".svg": "image/svg+xml",
    ".json": "application/json",
    ".csv": "text/csv",
    ".wasm": "application/wasm",
    ".dll": "application/octet-stream",
    ".dat": "application/octet-stream",
    ".pdb": "application/octet-stream",
    ".blat": "application/octet-stream"
  },
  "globalHeaders": {
    "X-Content-Type-Options": "nosniff"
  }
}